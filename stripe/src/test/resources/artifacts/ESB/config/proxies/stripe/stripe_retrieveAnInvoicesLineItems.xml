<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="stripe_retrieveAnInvoicesLineItems"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
   <target>
      <inSequence>
         <property name="customer" expression="json-eval($.customer)"/>
         <property name="invoiceId" expression="json-eval($.invoiceId)"/>
         <property name="endingBefore" expression="json-eval($.endingBefore)"/>
         <property name="limit" expression="json-eval($.limit)"/>
         <property name="startingAfter" expression="json-eval($.startingAfter)"/>
         <property name="subscription" expression="json-eval($.subscription)"/>
         <property name="apiKey" expression="json-eval($.apiKey)"/>
         <stripe.init>
            <apiKey>{$ctx:apiKey}</apiKey>
         </stripe.init>
         <stripe.retrieveAnInvoicesLineItems>
            <customer>{$ctx:customer}</customer>
            <invoiceId>{$ctx:invoiceId}</invoiceId>
            <endingBefore>{$ctx:endingBefore}</endingBefore>
            <limit>{$ctx:limit}</limit>
            <startingAfter>{$ctx:startingAfter}</startingAfter>
            <subscription>{$ctx:subscription}</subscription>
         </stripe.retrieveAnInvoicesLineItems>
         <respond/>
      </inSequence>
      <outSequence>
         <send/>
      </outSequence>
   </target>
   <startingAfter/>
</proxy>
                                	