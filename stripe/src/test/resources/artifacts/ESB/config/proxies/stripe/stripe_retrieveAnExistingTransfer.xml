<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="stripe_retrieveAnExistingTransfer"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="transferId" expression="json-eval($.transferId)"/>
            <property name="apiKey" expression="json-eval($.apiKey)"/>
            <stripe.init>
                <apiKey>{$ctx:apiKey}</apiKey>
            </stripe.init>
            <stripe.retrieveAnExistingTransfer>
                <transferId>{$ctx:transferId}</transferId>
            </stripe.retrieveAnExistingTransfer>
            <respond/>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
