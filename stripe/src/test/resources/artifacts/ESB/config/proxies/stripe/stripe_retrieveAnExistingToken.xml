<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="stripe_retrieveAnExistingToken"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="tokenId" expression="json-eval($.tokenId)"/>
            <property name="apiKey" expression="json-eval($.apiKey)"/>
            <stripe.init>
                <apiKey>{$ctx:apiKey}</apiKey>
            </stripe.init>
            <stripe.retrieveAnExistingToken>
                <tokenId>{$ctx:tokenId}</tokenId>
            </stripe.retrieveAnExistingToken>
            <respond/>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
