<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="stripe_deAuthorizeAnAccessToken"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="clientId" expression="json-eval($.clientId)"/>
            <property name="stripeUserId" expression="json-eval($.stripeUserId)"/>
            <property name="apiKey" expression="json-eval($.apiKey)"/>
            <stripe.init>
                <apiKey>{$ctx:apiKey}</apiKey>
            </stripe.init>
            <stripe.deAuthorizeAnAccessToken>
                <clientId>{$ctx:clientId}</clientId>
                <stripeUserId>{$ctx:stripeUserId}</stripeUserId>
            </stripe.deAuthorizeAnAccessToken>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
